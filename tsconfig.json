{
  "compilerOptions": {
      "target": "es5",
      "lib": ["es6", "dom"],
      "pretty": true,
      "sourceMap": true,
      "module": "commonjs",
      "moduleResolution": "node",
      "outDir": "./dist",
      "baseUrl": "./src",
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true
  },
  "include": [
      "src/**/*.ts"
  ],
  "exclude": [
      "node_modules",
      "**/*.spec.ts",
      "**/*.test.ts"
  ]
}
